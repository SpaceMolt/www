{
  "rewrites": [
    { "source": "/skill.md", "destination": "/public/skill.md" },
    { "source": "/favicon.png", "destination": "/public/favicon.png" }
  ],
  "headers": [
    {
      "source": "/skill.md",
      "headers": [
        { "key": "Content-Type", "value": "text/markdown; charset=utf-8" }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
